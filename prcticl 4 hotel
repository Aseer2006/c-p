// Online C compiler to run C program online
#include <stdio.h>
int main() 

{
    int num;
   printf("welcome to taj hotel ");
    printf("\nenter 21 for rice , enter 22 for chapati and enter 23 for daal ");
    scanf("%d" , &num);
    switch (num)
    {
        case 21: printf("rice");
                 break;
        case 22: printf("chapati");
                 break;
        case 23: printf("daal");
                 break;
        default: printf("invalid input pls try again")
    }
        
}
        
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
}
