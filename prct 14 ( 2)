// Online C compiler to run C program online
#include <stdio.h>
struct student 
{
    int  rollno;
    char name [20];
    int  marks;
}s[3];

 main()
{
    int i;
    for (i=0;i<3;i++)
    {
        printf("enter rollno,name and marks = ");
        scanf("%d %s %d" , & s[i].rollno , s[i].name ,&s[i].marks);
    }
    for (i=0;i<3;i++)
{
        printf("details of students \n");
        printf("\nrollno. is %d" ,s[i].rollno);
        printf("\nname is %s" , s[i].name);
        printf("\nmarks is %d" , s[i].marks);
    
}
    
}
