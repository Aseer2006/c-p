// Online C compiler to run C program online
#include <stdio.h>
void circle(float r)
{
    float d,c,a;
        d=2*r;
        c=2*3.14*r;
        a=3.14*r*r;
        printf("\nDiameter of cirlce is = %f" , d  );
        printf("\nCircumference of circle is = %f" , c);
        printf("\nArea of circle is = %f" , a);
}
void main()
{
    float radius;
    printf("enter radius of a circle = ");
    scanf("%f" , &radius);
    circle(radius);
}
