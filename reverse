#include<stdio.h>
void main() 
{
    int n;
    int num;
    int rev=0;
    printf("enter no. ");
    scanf("%d" , &n);
    while ( n>0)
    {
    num=n%10;
    printf(" %d" , num);
    n=n/10;
    }
    
}
// Online C compiler to run C program online
#include <stdio.h>
main()
{
    int i,r, n, rev=0, q;
    printf("enter no.");
    scanf("%d" , &n);
    
for (i=1 ;i<=n;i++)
{
    r=n%10;
    rev=rev*10+r;
    q=n/10;
    n=q;
}
    printf("enter no. is %d" , rev);
    
}
    
