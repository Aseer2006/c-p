#include <stdio.h>
main()
{
    char ch ;
    
       printf("enter character ");
       scanf("%c" , &ch);
       
       switch (ch)
    {
        case 'a':
        case 'e' :                              
        case 'i'  :                             
        case 'o'   :                            
        case 'u'    :
        case 'A'     :                          
        case 'E'      :                         
        case 'I'       :                        
        case 'O'        :                       
        case 'U'         :              
        printf("enter no. is VOWEL");
        break;
        
        
        default : printf("invalid input pls try again");
    }
}
